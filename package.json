{
	"name": "Hive",
	"version": "0.1.96",
	"versionDate": "2024-04-01",
	"author": "hackolade",
	"engines": {
		"hackolade": "6.1.2",
		"hackoladePlugin": "1.2.0"
	},
	"contributes": {
		"target": {
			"applicationTarget": "HIVE",
			"title": "Apache Hive",
			"versions": [
				"1.x",
				"2.x",
				"3.x"
			]
		},
		"features": {
            "views": {
                "enabled": true,
                "viewLevel": "model",
                "disablePipelines": true
            },
			"forwardEngineering": {
				"jsonSchema": {
					"keepParentType": {
						"date": {
							"type": "string",
							"format": "date"
						}
					}
				}
			},
			"nestedCollections": false,
			"disablePatternField": true,
			"enableForwardEngineering": true,
			"disableMultipleTypes": false,
			"enableReverseEngineering": true,
			"enableComplexTypesNormalization": true,
			"disableChoices": false,
			"enableJsonType": true,
			"relationships": {
                "compositeRelationships": true                
			},
			"FEScriptCommentsSupported": ["2.x", "3.x"],
			"discoverRelationships": true,
			"enableKeysMultipleAbrr": true
		}
	},
	"description": "Hackolade plugin for Apache Hive"
}
