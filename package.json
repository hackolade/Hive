{
    "name": "Hive",
    "version": "0.1.99",
    "versionDate": "2024-04-12",
    "author": "hackolade",
    "engines": {
        "hackolade": "6.1.2",
        "hackoladePlugin": "1.2.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "HIVE",
            "title": "Apache Hive",
            "versions": [
                "1.x",
                "2.x",
                "3.x"
            ]
        },
        "features": {
            "views": {
                "enabled": true,
                "viewLevel": "model",
                "disablePipelines": true
            },
            "forwardEngineering": {
                "jsonSchema": {
                    "keepParentType": {
                        "date": {
                            "type": "string",
                            "format": "date"
                        }
                    }
                }
            },
            "nestedCollections": false,
            "disablePatternField": true,
            "enableForwardEngineering": true,
            "disableMultipleTypes": false,
            "enableReverseEngineering": true,
            "enableComplexTypesNormalization": true,
            "disableChoices": false,
            "enableJsonType": true,
            "relationships": {
                "compositeRelationships": true
            },
            "FEScriptCommentsSupported": [
                "2.x",
                "3.x"
            ],
            "discoverRelationships": true,
            "enableKeysMultipleAbrr": true
        }
    },
    "description": "Hackolade plugin for Apache Hive",
    "dependencies": {
        "antlr4": "4.8.0",
        "async": "2.6.1",
        "big.js": "5.2.1",
        "sql-formatter": "4.0.2",
        "ssl-root-cas": "1.3.1",
        "thrift": "0.15.0",
        "uuid": "7.0.3"
    },
    "lint-staged": {
        "*.{js,json}": "prettier --write"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npx eslint ."
    },
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "package": "node esbuild.package.js"
    },
    "devDependencies": {
        "@hackolade/hck-esbuild-plugins-pack": "0.0.1",
        "esbuild": "0.21.4",
        "esbuild-plugin-clean": "1.0.1",
        "eslint": "9.3.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.1.3",
        "lint-staged": "14.0.1",
        "prettier": "3.2.5",
        "simple-git-hooks": "2.11.1"
    }
}
